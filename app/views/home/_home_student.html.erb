<%= javascript_pack_tag 'student_home' %>

<div id="react-app"></div>
<div class="container d-none" >
  <div class="row" >
    <div class="col-md-12 text-center fw-bold prompt1" >

      Ready to play? Remember, go as FAST as you can!!!
   
    </div>
  </div>

  <div class="row">
    <div class="col-md-12 text-center">
    <%= form_with(url: rounds_path) do |form| %>
    <%= form.label "How many exercises?" %>
    <%= form.text_field "query_round_length" %>
    <%= form.button "Go", class: "btn btn-dark" %>
    <% end %>
    <div>
  </div>
</div>
